{% extends 'template.html.twig' %}
{% block title %}Abonnement{% endblock %}
{% block body %}
<div class="page-inner mt--5">
					<div class="row">
						<div class="col-md-12">
							<div class="card">
								<div class="card-header">
									<div class="card-title">Consulter Abonnement</div>
									<div class="card-category">Finance> Abonnement > Consulter</div>
								</div>
   <div class="card-body">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ abonnement.id }}</td>
            </tr>
            <tr>
                <th>Etat</th>
                <td>{{ abonnement.etat ? 'Yes' : 'No' }}</td>
            </tr>
            <tr>
                <th>Transaction</th>
                <td>{{ abonnement.transaction }}</td>
            </tr>
            <tr>
                <th>DateEn</th>
                <td>{{ abonnement.dateEn ? abonnement.dateEn|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>DateExp</th>
                <td>{{ abonnement.dateExp ? abonnement.dateExp|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>
    </div>
    <div class="card-action">
        <div class="row">
            <div class="col-md-12">
                <a href="{{ path('abonnement_index') }}" class="btn btn-primary" ><i class="fa fa-arrow-circle-left"></i> Retourner Ã  la liste</a>

                <a href="{{ path('abonnement_edit', {'id': abonnement.id}) }}" class="btn btn-warning" ><i class="fa fa-edit"></i> Modifier</a>
            </div>										
        </div>
    </div>
   

    {{ include('abonnement/_delete_form.html.twig') }}


									
								
							</div>
						</div>
					</div>
					
				</div>
			
{% endblock %}
