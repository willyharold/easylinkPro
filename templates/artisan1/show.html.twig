{% extends 'template.html.twig' %}
{% block title %}Artisan{% endblock %}
{% block body %}
<div class="page-inner mt--5">
					<div class="row">
						<div class="col-md-12">
							<div class="card">
								<div class="card-header">
									<div class="card-title">Consulter Artisan</div>
									<div class="card-category">Paramètres > Artisan > Consulter</div>
								</div>
   <div class="card-body">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ artisan.id }}</td>
            </tr>
            <tr>
                <th>Nom</th>
                <td>{{ artisan.nom }}</td>
            </tr>
            <tr>
                <th>DateCreateAt</th>
                <td>{{ artisan.dateCreateAt ? artisan.dateCreateAt|date('Y-m-d') : '' }}</td>
            </tr>
            <tr>
                <th>Fiscale</th>
                <td>{{ artisan.fiscale }}</td>
            </tr>
            <tr>
                <th>Siren</th>
                <td>{{ artisan.siren }}</td>
            </tr>
            <tr>
                <th>Adresse</th>
                <td>{{ artisan.adresse }}</td>
            </tr>
            <tr>
                <th>CodePostal</th>
                <td>{{ artisan.codePostal }}</td>
            </tr>
            <tr>
                <th>Telephone</th>
                <td>{{ artisan.telephone }}</td>
            </tr>
            <tr>
                <th>Telephone2</th>
                <td>{{ artisan.telephone2 }}</td>
            </tr>
        </tbody>
    </table>
</div>
    <div class="card-action">
        <div class="row">
            <div class="col-md-12">
                <a href="{{ path('artisan_index') }}" class="btn btn-primary" ><i class="fa fa-arrow-circle-left"></i> Retourner à la liste</a>

                <a href="{{ path('artisan_edit', {'id': artisan.id}) }}" class="btn btn-warning" ><i class="fa fa-edit"></i> Modifier</a>
            </div>										
        </div>
    </div>
   

    {{ include('artisan/_delete_form.html.twig') }}


									
								
							</div>
						</div>
					</div>
					
				</div>
			
{% endblock %}
